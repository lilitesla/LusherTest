<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="16dp"
    tools:context="lili.tesla.lushertest.table3.view.Table3Activity">

    <TextView
        android:id="@+id/textView_description"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_better"/>

    <LinearLayout
        android:id="@+id/linearLayout_grid"
        android:layout_centerInParent="true"
        android:layout_width="match_parent"
        android:layout_height="360dp"
        android:orientation="horizontal"
        android:layout_marginTop="20dp"
        android:gravity="center">

            <ImageView
                android:id="@+id/image0_table3"
                style="@style/imageTable3" />

            <ImageView
                android:id="@+id/image1_table3"
                style="@style/imageTable3" />

            <ImageView
                android:id="@+id/image2_table3"
                style="@style/imageTable3" />

            <ImageView
                android:id="@+id/image3_table3"
                style="@style/imageTable3" />





    </LinearLayout>


</RelativeLayout>
