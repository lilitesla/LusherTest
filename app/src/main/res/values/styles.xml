<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="imageTable2">
        <item name="android:layout_height">85dp</item>
        <item name="android:layout_width">140dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="imageTable2land">
        <item name="android:layout_height">100dp</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="linearLayoutTable2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="imageTable3land">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">180dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="imageTable3">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">140dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="imageTable1land">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="imageTable1">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">200dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="linearlayoutTable1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">220dp</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="TextViewDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>

</resources>
